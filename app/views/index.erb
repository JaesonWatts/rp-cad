<div class="container">

  <% if logged_in && current_user.admin == 0%>
    <h2>Welcome <%= current_user.username %></h2>
    <h2>Your admin level is <%= current_user.admin_level %> </h2>
    <a href="/logout">Logout</a> | <a href="/persona/show">View Your Personas </a> 
  <% elsif logged_in && current_user.admin >= 1%>
    <h2>Welcome <%= current_user.username %></h2>
    <h2>Your admin level is <%= current_user.admin_level %> </h2>
    <a href="/logout">Logout</a> | <a href="/persona/show">View Your Personas</a> | <a href="/members/index.html">View all Members</a>
  <% else  %>
    <h2>Please log in first</h2>
    <a href="/signup">Signup</a> | <a href="/login">Login</a>
  <% end %>
</div>