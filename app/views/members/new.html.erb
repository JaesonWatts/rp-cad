<h2>Create a new member</h2>

<form action="/members" method="post">
  <label for="name">Name:</label>
  <input type="text" name="member[name]">
  <br />
  <label for="admin_level">Admin Level</label>
    <select id = "admin_level">
      <option value = "0">Not an admin</option>
      <option value = "1">Community Director</option>
      <option value = "2">Deputy Director</option>
      <option value = "3">Advanced Roleplay Team</option>
      </select>
      <input type="submit" id="Submit">
    <%binding.pry%>
    
    

